<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>RegictServlet</servlet-name>
    <servlet-class>com.tedu.servlet.RegictServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>VlidateServlet</servlet-name>
    <servlet-class>com.tedu.servlet.VlidateServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AjaxCheckUsername</servlet-name>
    <servlet-class>com.tedu.servlet.AjaxCheckUsername</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.tedu.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogOutServlet</servlet-name>
    <servlet-class>com.tedu.servlet.LogOutServlet</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>RegictServlet</servlet-name>
    <url-pattern>/RegictServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VlidateServlet</servlet-name>
    <url-pattern>/VlidateServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AjaxCheckUsername</servlet-name>
    <url-pattern>/AjaxCheckUsername</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogOutServlet</servlet-name>
    <url-pattern>/LogOutServlet</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
